                    <div class="download-app1-images">
                        <img src="~/assets/img/gallery/@Model.GooglePlay" alt="">
                    </div>
                </div>
            </div>
        </div>
        <div class="d-flex flex-wrap footer-line mt-5">
            <div class="col-lg-7 mt-3">
                <div class="info-page">
                    <p>
                        Copyright ©2020 All rights reserved | This template is made with <i class="fas fa-heart"></i> by
                        <span>Sunal</span>
                    </p>
                </div>
            </div>
            <div class="col-lg-5 mb-1">
                <div class="icons-sosial d-flex">
                    <div class="border-icon">
                        <i class="@Model.Facebook"></i>
                    </div>
                    <div class="border-icon">
                        <i class="@Model.Twitter"></i>
                    </div>
                    <div class="border-icon">
                        <i class="@Model.Whatsapp"></i>
                    </div>
                    <div class="border-icon">
                        <i class="@Model.Instagram"></i>
                    </div>
                </div>
            </div>
