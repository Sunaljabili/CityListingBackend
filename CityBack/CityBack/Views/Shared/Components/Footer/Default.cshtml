                    <a href="index.html">
                        <img src="~/assets/img/logo/@Model.Logo" alt="">
                    </a>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6 col-6">
                <div class="footer-title ">
                    <h3>
                        Quick Link
                    </h3>
                    <ul class="footer-links mr-5">
                        <li>
                            <a asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li>
                            <a asp-controller="Listing" asp-action="Index">Listing</a>
                        </li>
                        <li>
                            <a asp-controller="About" asp-action="Index">About</a>
                        </li>
                        <li>
                            <a asp-controller="Contact" asp-action="Index">Contact</a>
                        </li>

                    </ul>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6 col-6">
                <div class="footer-title">
                    <h3>
                        Categories
                    </h3>
                    <ul class="footer-links">
                        <li>
                            <a href="#"> Hotel</a>
                        </li>
                        <li>
                            <a href="#"> Restaurant</a>
                        </li>
                        <li>
                            <a href="#">
                                Shopping
                            </a>
                        </li>
                        <li>
                            <a href="#">Night Life</a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6 col-6">
                <div class="footer-title">
                    <h3>Download App</h3>
                </div>
                <div class="app-img">
                    <div class="download-app1-images">
                        <img src="~/assets/img/gallery/@Model.AppStore" alt="">
