@model Explore
@{
    ViewData["Title"] = "Delete";
}
    <div class="row">
        <div class="col-12 mt-5">

            <h3>@Model.ChooseCategory</h3>
            <div>@Html.Raw(Model.CategoryTitle)</div>
            <div class="text-danger">Are You Sure?</div>
        </div>
        <div class="d-flex justify-content-end align-items-center">
            <a  asp-action="Index" class="btn btn-dark mt-2">Go Back</a>
            <form  method="post">
                <input type="submit" class="btn btn-danger mt-2 ml-2"  value="Delete Explore" />
            </form>
        </div>
    </div>


